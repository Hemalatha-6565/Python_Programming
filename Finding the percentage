if __name__ == '__main__':
   # number of students
   n = int(input())

student_marks = {}

# read student data
for _ in range(n):
    data = input().split()
    name = data[0]
    marks = list(map(float, data[1:]))
    student_marks[name] = marks

# student to query
query_name = input()

# calculate average
marks_list = student_marks[query_name]
average = sum(marks_list) / len(marks_list)

# print result with 2 decimal places
print(f"{average:.2f}")
