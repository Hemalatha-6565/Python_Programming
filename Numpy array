import numpy
 
def arrays(arr):
    # complete this function
    # use numpy.array
    # Convert list to numpy array of floats and reverse it
    return numpy.array(arr, float)[::-1]
    # Returns reversed NumPy array
    #[::-1] is used in [start : end : step],this is array slicing reverse
arr = input().strip().split(' ')
result = arrays(arr)
print(result)
