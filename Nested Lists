if __name__ == '__main__':
   students = []   # to store student name and grade

n = int(input())   # number of students

# Read input
for i in range(n):
    name = input()          # student name
    grade = float(input())  # student grade
    students.append([name, grade])

# Find the lowest grade
lowest = students[0][1]  # 0 is the first student,1 isthe student's mark

for student in students:
    if student[1] < lowest:
        lowest = student[1]

# Find the second lowest grade
second_lowest = float('inf')    # float('inf') means very very big number

for student in students:
    if student[1] > lowest and student[1] < second_lowest:
        second_lowest = student[1]

# Collect names with second lowest grade
names = []

for student in students:
    if student[1] == second_lowest:
        names.append(student[0])

# Sort names alphabetically
names.sort()

# Step 6: Print names
for name in names:
    print(name)

      
